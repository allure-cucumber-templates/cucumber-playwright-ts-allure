{
  "name": "cucumber-playwright",
  "version": "1.0.0",
  "description": "Starter project to write and debug cucumber-js features with Playwright",
  "main": "index.js",
  "scripts": {
    "api": "DEBUG=pw:api cucumber-js",
    "build": "rimraf build && npm run format && npm run lint && tsc && npm run cucumber-check",
    "cucumber-check": "cucumber-js features/**/*.feature --dry-run --require env/set-environment-variables.ts --require world/custom-world.ts --require step-definitions/**/*.ts --require hooks/**/*.ts  --require-module ts-node/register --format-options \"{\\\"snippetInterface\\\": \\\"async-await\\\"}\" --format summary --format progress --format progress-bar  --publish-quiet",
    "debug": "PWDEBUG=1 DEBUG=pw:api cucumber-js",
    "eslint-fix": "eslint ./ --ext .js,.ts,.tsx,.json --fix",
    "eslint-init": "eslint --init",
    "format": "prettier --write \"**/*.{ts,tsx,css,html,json}\" ",
    "lint": "eslint ./ --ext .js,.ts,.tsx --format visualstudio --no-color --report-unused-disable-directives",
    "only": " cucumber-js -- --tags @only",
    "report": "open reports/report.html",
    "snippets": "cucumber-js features/**/*.feature --dry-run --format snippets",
    "steps-usage": "cucumber-js features/**/*.feature --dry-run",
    "env": "env",
    "feature": "cucumber-js --config ./conf/cucumber.mjs",
    "tags": "cucumber-js --config ./conf/cucumber.mjs features/**/*.feature",
    "test:ui": "cucumber-js --config ./conf/cucumber.mjs features/**/*.feature --tags=@ui",
    "test:api": "cucumber-js --config ./conf/cucumber.mjs features/**/*.feature --tags @api",
    "test:data": "cucumber-js --config ./conf/cucumber.mjs features/**/*.feature --tags @data",
    "test:perf": "cucumber-js --config ./conf/cucumber.mjs features/**/*.feature --tags @perf",
    "test:acessibility": "cucumber-js --config ./conf/cucumber.mjs features/**/*.feature --tags @acessibility",
    "test:all": "cucumber-js --config ./conf/cucumber.mjs features/**/*.feature",
    "test:debug": "cucumber-js --config ./conf/cucumber.mjs --require conf/reporter/allure-reporter.ts features/**/*.feature --format summary --format progress-bar",
    "allure:serve": "allure serve reports/allure-results",
    "allure:generate": "allure generate reports/allure-results --clean -o reports/allure-report",
    "clean:results:unix": "rm -rf reports/* && mkdir -p reports | true",
    "clean:results:win32": "rmdir /s /q reports && mkdir reports"
  },
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "@azure/msal-node": "^1.14.5",
    "@cucumber/cucumber": "8.5.2",
    "@cucumber/html-formatter": "20.0.0",
    "@cucumber/messages": "19.0.0",
    "@cucumber/pretty-formatter": "1.0.0",
    "@playwright/test": "1.25.1",
    "@types/fs-extra": "9.0.13",
    "allure-cucumberjs": "^2.0.0-beta.19",
    "axios": "^0.26.0",
    "csv-parse": "^5.0.4",
    "cucumber-console-formatter": "1.0.0",
    "dotenv": "^16.0.0",
    "expect": "27.5.1",
    "graphql-request": "5.0.0",	
    "ll": "^1.2.0",
    "mssql": "^9.0.0",
    "mysql2": "^2.3.3",
    "pixelmatch": "5.2.1",
    "playwright": "1.25.1",
    "pngjs": "6.0.0",
    "requirements": "^1.4.1",
    "testcontainers": "^8.13.0",
    "husky": "^8.0.1"
  },
  "devDependencies": {
    "@types/ioredis": "4.28.10",
    "@types/expect": "^24.3.0",
    "@types/lodash": "4.14.184",
    "@types/mssql": "8.1.0",
    "@azure/msal-node": "^1.14.5",
    "@types/node": "16.11.26",
    "@types/pixelmatch": "5.2.4",
    "@types/pngjs": "6.0.1",
    "@typescript-eslint/eslint-plugin": "5.36.0",
    "@typescript-eslint/parser": "5.36.0",
    "allure-commandline": "^2.17.2",
    "allure-cucumberjs": "2.0.0-beta.19",
    "eslint": "8.13.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "fs-extra": "10.1.0",
    "open": "8.4.0",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "standard-version": "9.5.0",
    "ts-node": "10.9.1",
    "typescript": "^4.8.2",
    "tedious": "15.1.0"
  },
  "lint-staged": {
    "**/*.ts": [
      "prettier --write"
    ]
  }
}
